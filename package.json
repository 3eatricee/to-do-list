		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@trpc/client": "^11.0.4",
		"@trpc/server": "^11.0.4",
		"@types/node": "^22.14.0",
		"dotenv": "^16.4.7",
		"drizzle-kit": "^0.30.6",
		"drizzle-orm": "^0.41.0",
		"drizzle-zod": "^0.7.1",
		"postgres": "^3.4.5",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"superjson": "^2.2.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.5",
		"zod": "^3.24.2"
	},
	"dependencies": {
		"@iconify/svelte": "^4.2.0"
	}
